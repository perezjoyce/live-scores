{
  "compilerOptions": {
    "jsx": "react-jsx",
    "resolveJsonModule": true,
    "baseUrl": "src/",
    "paths": {
      "@/types/*": [
        "types/*"
      ],
      "@/redux/*": [
        "redux/*"
      ],
      "@/data/*": [
        "data/*"
      ],
      "@/assets/*": [
        "assets/*"
      ],
      "@/app/*": [
        "app/*"
      ],
      "@/components/*": [
        "app/components/*"
      ],
      "@/common/*": [
        "app/components/common/*"
      ],
      "@/utils/*": [
        "app/utils/*"
      ],
    }
  }
}